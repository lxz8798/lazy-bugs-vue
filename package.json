{
  "name": "app2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development --progress --color",
    "build": "vue-cli-service build --mode production --progress --color",
    "build:pro": "vue-cli-service build --mode production --progress --color & xcopy /fy \"./config/index.js\" \"dist/config.js\"",
    "demo:demo": "vue-cli-service build --mode production --progress --color & xcopy /fy \"./config/demo.js\" \"dist/config.js\"",
    "test": "vue-cli-service serve --mode test",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "amfe-flexible": "^2.2.1",
    "axios": "^0.19.2",
    "echarts": "^4.8.0",
    "glob": "^7.1.3",
    "iview": "^3.1.3",
    "lib-flexible": "^0.3.2",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.4",
    "normalize.css": "^8.0.0",
    "postcss-pxtorem": "^5.1.1",
    "sass-loader": "^7.1.0",
    "vant": "^1.6.17",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "@vue/standard"
    ],
    "rules": {
      "generator-star-spacing": "off",
      "no-unused-vars": "off",
      "quotes": "off"
    },
    "parserOptions": {
